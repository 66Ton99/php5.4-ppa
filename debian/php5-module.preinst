#!/bin/sh

set -e

#EXTRA#
#DEBHELPER#

for dsoname in @modules@; do
    inifile=${dsoname}.ini

    if [ -x /usr/bin/dpkg-maintscript-helper ]; then
	dpkg-maintscript-helper mv_conffile \
	    /etc/php5/conf.d/${inifile} \
	    /etc/php5/mods-available/${inifile} \
	    5.4.0~rc6-1 -- "$@";
    fi
done

exit 0
